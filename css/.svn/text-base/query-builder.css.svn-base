#query{
	position:fixed;  
	top:230px; 
	right:-32px;
	z-index:9999;
}
#query a {
	display:block;
	height:15px;
	width:80px;
	text-align:center;
	background:#65944A;
	/*border:1px solid #fff;*/
	outline:1px solid #a1a1a1;
	padding:5px;
	float:left;
	cursor:pointer;
	/*Font*/
	color:#FFF;
	font-weight:bold;
	font-size:13px;
	opacity: 0.85;
		
	/* Safari */
	-webkit-transform: rotate(-90deg);

	/* Firefox */
	-moz-transform: rotate(-90deg);

	/* IE */
	-ms-transform: rotate(-90deg);

	/* Opera */
	-o-transform: rotate(-90deg);

	/* Internet Explorer */
	filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);
	
	
}
#query a.open  {
	position:fixed; 
	/*text-align:left;*/
	top:230px; 
	right:258px;
}
#query a:hover {
	height:16px;
	padding-bottom:6px;
}

#query_form .filter-btn {
	-moz-box-shadow:inset 0px 1px 0px 0px #ffffff;
	-webkit-box-shadow:inset 0px 1px 0px 0px #ffffff;
	box-shadow:inset 0px 1px 0px 0px #ffffff;
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #ededed), color-stop(1, #dfdfdf) );
	background:-moz-linear-gradient( center top, #ededed 5%, #dfdfdf 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ededed', endColorstr='#dfdfdf');
	background-color:#ededed;
	-moz-border-radius:6px;
	-webkit-border-radius:6px;
	border-radius:6px;
	border:1px solid #dcdcdc;
	display:inline-block;
	color:#777777;
	font-family:arial;
	font-size:10px;
	font-weight:bold;
	padding:3px 13px;
	text-decoration:none;
	text-shadow:1px 1px 0px #ffffff;
}
#query_form .filter-btn:hover {
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #dfdfdf), color-stop(1, #ededed) );
	background:-moz-linear-gradient( center top, #dfdfdf 5%, #ededed 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#dfdfdf', endColorstr='#ededed');
	background-color:#dfdfdf;
}
#query_form .filter-btn:active {
	position:relative;
	top:1px;
}

#query_form .form{
	position:fixed;  
	top:0px; 
	right:500px;
	z-index:9999;
	clear:both;
	height:500000px;
	width:259px;
	right:-5px;
	/*border:1px solid #fff;*/
	/*outline:1px solid #a1a1a1;*/
	border-right:6px solid #65944A;
	background:#F5F5F5;
	/*background:#65944A;*/
	/*padding:5px;*/
	display: none;
	opacity: 0.96;
}
#query_form .form h1{
	font-family:'TitilliumText22L',Arial,sans-serif;
	font-size: 14px;
	color: #65944A;
	padding: 10px 0px 0px 0px;
}
#query_form .form h2{
    font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
	color: #666;
    font-size: 13px;
    font-weight: 700;
}
#query_form .form .dot{
	width: 8px; 
	height: 8px; 
	display: block; 
	background: #666; 
	background: rgba(0,0,0,0.2); 
	cursor: pointer; 
	text-indent: -9999px; 
	-webkit-border-radius: 20px; 
	-moz-border-radius: 20px; 
	-o-border-radius: 20px; 
	border-radius: 20px; 
	box-shadow: inset 0 0 3px rgba(0,0,0,0.3);
}
#query_form .form .dot:hover{
	background: #333; 
	background: rgba(0,0,0,0.6);
}

#query_form .form .block {
    /*background: none repeat scroll 0 0 #F4F4F0;*/
    border-color: #E4E4E0 #F5F5F5;
    border-style: solid;
    border-width: 1px;
    margin: -1px 0 0;
    padding: 0px 10px 10px 0px;
}
#query_form .form .block h3.block-title {
    font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
	color: #666;
    font-size: 11px;
    font-weight: 700;
}
#query_form .form .block .searchbox {
    position: relative;
}
#query_form .form .block .searchbox .search {
    font-size: 12px;
    line-height: 12px;
    margin-top: 2px;
	margin-left: 13px;
    padding: 6px 30px 6px 6px;
    width: 195px;
}
#query_form .form .block .searchbox .search-button {
    background: url('../images/icon-search.png') no-repeat scroll 0 0 transparent;
    cursor: pointer;
    display: block;
    height: 20px;
    position: absolute;
    right: 5px;
    text-indent: -9999px;
    top: 9px;
    width: 20px;
	border:none;
    z-index: 99;
}
ul, ul li {
    list-style: none outside none;
}
#query_form .form .block .item-list ul  li {
    background: none repeat scroll 0 0 transparent;
    margin: 3px 0;
}
#query_form .form .block .item-list .checkbox-links {
    font-size: 12px;
    padding: 5px 0 0 0;
}
#query_form .form .block .search-checkbox {
    margin-right: 0.5em;
}
#query_form .form .block .search-checkbox-label {
	cursor: pointer;
	font-weight:normal;
    color: #0988BB;
    text-decoration: none;
}
#query_form .form .block .search-checkbox:checked + .search-checkbox-label {
	color: #666;
    font-size: 13px;
    font-weight: bold;
}
#query_form .form .block .search-checkbox-label:hover {
    text-decoration: underline;
}
#query_form .form .block .dropdown {
    padding: 0 0 0 14px;
}